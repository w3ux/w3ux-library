{
  "name": "@w3ux/utils-source",
  "description": "A collection of reusable utilities for manipulating data",
  "version": "2.0.10",
  "license": "GPL-3.0-only",
  "type": "module",
  "keywords": [
    "w3ux",
    "polkadot",
    "web3",
    "utilities",
    "typescript",
    "data-manipulation"
  ],
  "homepage": "https://w3ux.org/library/utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3ux/w3ux-library.git",
    "directory": "library/utils"
  },
  "bugs": {
    "url": "https://github.com/w3ux/w3ux-library/issues"
  },
  "scripts": {
    "clear": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "test": "vitest run",
    "build": "tsup src/**/* --format esm,cjs --target es2022 --dts --no-splitting",
    "compile": "builder -t utils"
  },
  "dependencies": {
    "dedot": "^0.13.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.12",
    "builder": "workspace:*",
    "react": "^19.1.0",
    "tsup": "^8.4.0",
    "vitest": "^3.1.1"
  }
}

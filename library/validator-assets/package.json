{
  "name": "@w3ux/validator-assets-source",
  "description": "A list of Polkadot validator operators with metadata and icons",
  "version": "1.1.0",
  "license": "GPL-3.0-only",
  "type": "module",
  "keywords": [
    "w3ux",
    "polkadot",
    "validators",
    "assets",
    "icons",
    "operators",
    "react",
    "typescript"
  ],
  "homepage": "https://w3ux.org/library/validator-assets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3ux/w3ux-library.git",
    "directory": "library/validator-assets"
  },
  "bugs": {
    "url": "https://github.com/w3ux/w3ux-library/issues"
  },
  "scripts": {
    "clear": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "build": "tsup src/**/*{.ts,.tsx} --format esm,cjs --target es2022 --dts --no-splitting",
    "compile": "builder -t validator-assets"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@types/react": "^19.0.12",
    "builder": "workspace:*",
    "react": "^19.1.0",
    "tsup": "^8.4.0"
  }
}

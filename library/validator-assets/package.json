{
  "name": "@w3ux/validator-assets-source",
  "license": "GPL-3.0-only",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clear": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "compile": "tsup src/**/*{.ts,.tsx} --format esm,cjs --target es2022 --dts --no-splitting",
    "build": "builder -t validator-assets"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@types/react": "^19.0.12",
    "builder": "workspace:*",
    "react": "^19.1.0",
    "tsup": "^8.4.0"
  }
}
